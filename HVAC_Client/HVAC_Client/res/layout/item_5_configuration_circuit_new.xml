<?xml version		= "1.0" encoding			= "utf-8"?>
<LinearLayout 
    xmlns:android								= "http://schemas.android.com/apk/res/android"
    style										= "@style/AD_Linear_Top"
>

    <include layout								= "@layout/aa_include_panel_title_section" />

    <LinearLayout
        android:layout_width					= "match_parent"
        android:layout_height					= "match_parent"
        android:background						= "@color/background"
        android:orientation						= "vertical" 
	>
    	<RelativeLayout
    		android:layout_width				= "fill_parent" 
    		android:layout_height				= "wrap_content"
    	>
	        <TextView
	            style							= "@style/AD_Text"
	            android:text					= "Pump"
	            android:layout_alignParentLeft	= "true" 
			/>
	        <TextView
	            android:id						= "@+id/pumpName"
	            style							= "@style/AD_Data"
	            android:layout_alignParentRight	= "true" 
			/>
	    </RelativeLayout>
    	<RelativeLayout
    		android:layout_width				= "fill_parent" 
    		android:layout_height				= "wrap_content"
    	>
	        <TextView
	            style							= "@style/AD_Text"
	            android:text					= "Thermometer"
	            android:layout_alignParentLeft	= "true" 
           
			/>
	        <TextView
	            android:id						= "@+id/thermometerName"
	            style							= "@style/AD_Data"
	            android:layout_alignParentRight	= "true" 
			/>
	    </RelativeLayout>
	    <LinearLayout
	        android:id							= "@+id/gradient"
	        android:layout_width				= "match_parent"
	        android:layout_height				= "wrap_content" 
	    	android:orientation					= "vertical"
		>
	
	        <TextView
	            style							= "@style/AD_Text"
	            android:layout_width			= "match_parent"
	            android:gravity					= "center_horizontal"
	            android:text					= "Temperature Gradient"
			/>

	        <LinearLayout
		        android:layout_width			= "match_parent"
		        android:layout_height			= "wrap_content" 
		    	android:orientation				= "vertical"
			>
	        
				<ImageView
				    android:id					= "@+id/imageView1"
				    android:layout_width		= "180dp"
				    android:layout_height		= "110dp"
				    android:layout_gravity		= "center"
				    android:maxWidth			= "180dp"
				    android:src					= "@drawable/gradient"
				/>

				<GridLayout
		            android:layout_width			= "match_parent"
		            android:layout_height			= "wrap_content" 
		            android:columnCount				= "5"
		            android:rowCount				= "5"
				>
			        <!-- Row 0 : Circuit Temperatures -->
			        <TextView
			            style						= "@style/AD_Text"
			            android:layout_row			= "0"
			            android:layout_columnSpan	= "2"
			            android:text				= "Circuit T°"
					/>
			        <TextView
			            android:id					= "@+id/tempLow"
			            style						= "@style/AD_Data"
			            android:text				= "low"
					/>
			        <TextView
			            style						= "@style/AD_Data"
			            android:text				= ""
					/>
					<TextView
					    android:id					= "@+id/tempHigh"
					    style						= "@style/AD_Data"
					    android:text				= "high"
					/>
			        			        			        			        
			        
			        
			        
			        <!-- Row 1 : Outside Temperatures -->
			        			        <TextView
			            style						= "@style/AD_Text"
			            android:layout_row			= "1"
			            android:layout_columnSpan	= "2"
			            android:text				= "Outside T°"
					/>
					<TextView
					    android:id					= "@+id/outsideLow"
					    style						= "@style/AD_Data"
					    android:text				= "OS Low"
					/>
			        <TextView
			            style						= "@style/AD_Data"
			            android:text				= ""
					/>
					<TextView
					    android:id					= "@+id/outsideHigh"
					    style						= "@style/AD_Data"
					    android:text				= "OS High"
					/>
								        			        
				</GridLayout>
			</LinearLayout>
	    </LinearLayout>
	    <LinearLayout
	        android:id							= "@+id/mixer"
	        android:layout_width				= "match_parent"
	        android:layout_height				= "wrap_content" 
	    	android:orientation					= "vertical"
		>
	
	         <TextView
	             style							= "@style/AD_Text"
	             android:layout_width			= "match_parent"
	             android:gravity				= "center_horizontal"
	             android:text					= "Mixer"
			/>
	
	         <LinearLayout
	             android:id						= "@+id/LinearLayout05"
	             android:layout_width			= "wrap_content"
	             android:layout_height			= "wrap_content"
	             android:orientation			= "horizontal"
			>
	
	            <TextView
	                 style						= "@style/AD_Text"
	                 android:layout_width		= "150dp"
	                 android:gravity			= "left"
	                 android:text				= "Thermometer"
				/>
	
	             <TextView
	                 android:id					= "@+id/thermometer"
	                 style						= "@style/AD_Data"
	                 android:layout_width		= "150dp"
	                 android:gravity			= "right"
	                 android:text				= "zz"
				/>
	         </LinearLayout>
	
	         <LinearLayout
	             android:id						= "@+id/LinearLayout03"
	             android:layout_width			= "wrap_content"
	             android:layout_height			= "wrap_content"
	             android:orientation			= "horizontal"
			>
	
	            <TextView
	                 style						= "@style/AD_Text"
	                 android:layout_width		= "150dp"
	                 android:gravity			= "left"
	                 android:text				= "Swing Time"
				/>
	
	            <TextView
	                 android:id					= "@+id/swingTime"
	                 style						= "@style/AD_Data"
	                 android:layout_width		= "150dp"
	                 android:gravity			= "right"
	                 android:text				= "zz"
				/>
	
	         </LinearLayout>
	
	         <LinearLayout
	             android:id						= "@+id/LinearLayout04"
	             android:layout_width			= "wrap_content"
	             android:layout_height			= "wrap_content"
	             android:orientation			= "horizontal"
			>
	
	             <TextView
	                 style						= "@style/AD_Text"
	                 android:layout_width		= "150dp"
	                 android:gravity			= "left"
	                 android:text				= "Relay Up"
				/>
	
	            <TextView
	                 android:id					= "@+id/relayUp"
	                 style						= "@style/AD_Data"
	                 android:layout_width		= "150dp"
	                 android:gravity			= "right"
	                 android:text				= "zz"
				/>
			</LinearLayout>
			<LinearLayout
	             android:layout_width			= "wrap_content"
	             android:layout_height			= "wrap_content"
	             android:orientation			= "horizontal"
			>
	
	             <TextView
	                 style						= "@style/AD_Text"
	                 android:layout_width		= "150dp"
	                 android:gravity			= "left"
	                 android:text				= "Relay Down"
				/>
	
	             <TextView
	                 android:id					= "@+id/relayDown"
	                 style						= "@style/AD_Data"
	                 android:layout_width		= "150dp"
	                 android:gravity			= "right"
	                 android:text				= "zz"
				/>
	         </LinearLayout>
	    </LinearLayout>
		<LinearLayout
		    android:id							= "@+id/pidMixer"
		    android:layout_width				= "match_parent"
		    android:layout_height				= "wrap_content"
		    android:orientation					= "vertical"
		>
		    <TextView
		        style							= "@style/AD_Text"
		        android:layout_width			= "match_parent"
		        android:gravity					= "center_horizontal"
		        android:text					= "Low Level PID (Mixer)"
			/>
		    <LinearLayout
		        android:id						= "@+id/LinearLayout07"
		        android:layout_width			= "wrap_content"
		        android:layout_height			= "wrap_content"
		        android:orientation				= "horizontal"
			>
		        <TextView
		            style						= "@style/AD_Text"
		            android:layout_width		= "200dp"
		            android:gravity				= "left"
		            android:text				= "Thermometer"
				/>
		        <TextView
		            android:id					= "@+id/thermoName"
		            style						= "@style/AD_Data"
		            android:layout_width		= "100dp"
		            android:gravity				= "right"
		            android:text				= "zz"
				/>
		    </LinearLayout>
		    <LinearLayout
		        android:id						= "@+id/LinearLayout06"
		        android:layout_width			= "wrap_content"
		        android:layout_height			= "wrap_content"
		        android:orientation				= "horizontal"
			>
		        <TextView
		            style						= "@style/AD_Text"
		            android:layout_width		= "200dp"
		            android:gravity				= "left"
		            android:text				= "Proportional Gain"
				/>
		        <TextView
		            android:id					= "@+id/gainP"
		            style						= "@style/AD_Data"
		            android:layout_width		= "100dp"
		            android:gravity				= "right"
		            android:text				= "zz"
				/>
		    </LinearLayout>
		    <LinearLayout
		        android:id						= "@+id/LinearLayout09"
		        android:layout_width			= "wrap_content"
		        android:layout_height			= "wrap_content"
		        android:orientation				= "horizontal"
			>
	
		        <TextView
		            style						= "@style/AD_Text"
		            android:layout_width		= "220dp"
		            android:gravity				= "left"
		            android:text				= "Differential Time Const"
				/>
	
		        <TextView
		            android:id					= "@+id/timeD"
		            style						= "@style/AD_Data"
		            android:layout_width		= "80dp"
		            android:gravity				= "right"
		            android:text				= "zz"
				/>
		    </LinearLayout>
		    <LinearLayout
		        android:layout_width			= "wrap_content"
		        android:layout_height			= "wrap_content"
		        android:orientation				= "horizontal"
			>
				<TextView
		            style						= "@style/AD_Text"
		            android:layout_width		= "220dp"
		            android:gravity				= "left"
		            android:text				= "Integration Time Const"
				/>
	
		        <TextView
		            android:id					= "@+id/timeI"
		            style						= "@style/AD_Data"
		            android:layout_width		= "80dp"
		            android:gravity				= "right"
		            android:text				= "zz"
				/>
		    </LinearLayout>
			<LinearLayout
		        android:layout_width			= "wrap_content"
		        android:layout_height			= "wrap_content"
		        android:orientation				= "horizontal"
			>
	
		        <TextView
		            style						= "@style/AD_Text"
		            android:layout_width		= "250dp"
		            android:gravity				= "left"
		            android:text				= "Time Delay after decision"
				/>
	
		        <TextView
		            android:id					= "@+id/timeDelay"
		            style						= "@style/AD_Data"
		            android:layout_width		= "50dp"
		            android:gravity				= "right"
		            android:text				= "zz"
				/>
		    </LinearLayout>
		    <LinearLayout
		        android:layout_width			= "wrap_content"
		        android:layout_height			= "wrap_content"
		        android:orientation				= "horizontal"
			>
	
		        <TextView
		            style						= "@style/AD_Text"
		            android:layout_width		= "250dp"
		            android:gravity				= "left"
		            android:text				= "Decision Time Projection"
				/>
	
		        <TextView
		            android:id					= "@+id/timeProjection"
		            style						= "@style/AD_Data"
		            android:layout_width		= "50dp"
		            android:gravity				= "right"
		            android:text				= "zz"
				/>
		    </LinearLayout>
		    <LinearLayout
		        android:layout_width			= "wrap_content"
		        android:layout_height			= "wrap_content"
		        android:orientation				= "horizontal"
			>
	
		        <TextView
		            style						= "@style/AD_Text"
		            android:layout_width		= "250dp"
		            android:gravity				= "left"
		            android:text				= "Tmperature Error Margin"
				/>
	
		        <TextView
		            android:id					= "@+id/marginProjection"
		            style						= "@style/AD_Data"
		            android:layout_width		= "50dp"
		            android:gravity				= "right"
		            android:text				= "zz"
				/>
		    </LinearLayout>
		</LinearLayout>
	</LinearLayout>
</LinearLayout>
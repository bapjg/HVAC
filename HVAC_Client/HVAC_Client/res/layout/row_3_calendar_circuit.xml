<?xml version= "1.0" encoding= "utf-8"?>
<LinearLayout 
    xmlns:android					="http://schemas.android.com/apk/res/android"
    android:id						= "@+id/row"
    android:layout_width			= "288dp"
    android:layout_height			= "wrap_content"
    android:gravity					= "center_vertical"
    android:orientation				= "vertical"
    android:descendantFocusability	= "blocksDescendants"
  >
   	<TextView
        android:id					= "@+id/days"
        style						= "@style/AD_Text"
 		android:gravity				= "center_horizontal"
    	android:layout_width		= "288dp"
   	/>
   	<LinearLayout 
    	android:layout_width	 	= "match_parent"
    	android:layout_height	 	= "40dp"
    	android:orientation		 	= "horizontal"
        android:id			 		= "@+id/daySlots"
	>
 	    <!-- Days of the week -->
   	    
    </LinearLayout>
   	<LinearLayout 
	    android:layout_width		= "match_parent"
	    android:layout_height		= "wrap_content"
	    android:orientation			= "horizontal"
	>
	    <!-- Hours of the day for time bar -->
	    <TextView
        	android:text			= "0"
	        style					= "@style/AD_Text"
  			android:gravity			= "left"
	        android:layout_height	= "wrap_content"
	    	android:layout_width	= "36dp"
    	/>
	    <TextView
        	android:text			= "06"
	        style					= "@style/AD_Text"
  			android:gravity			= "center_horizontal"
	        android:layout_height	= "wrap_content"
	    	android:layout_width	= "72dp"
    	/>
	    <TextView
        	android:text			= "12"
 	        style					= "@style/AD_Text"
  			android:gravity			= "center_horizontal"
	        android:layout_height	= "wrap_content"
	    	android:layout_width	= "72dp"
    	/>
	    <TextView
        	android:text			= "18"
 	        style					= "@style/AD_Text"
  			android:gravity			= "center_horizontal"
	        android:layout_height	= "wrap_content"
	    	android:layout_width	= "72dp"
    	/>
	    <TextView
        	android:text			= "24"
 	        style					= "@style/AD_Text"
  			android:gravity			= "right"
	        android:layout_height	= "wrap_content"
	    	android:layout_width	= "36dp"
    	/>
	    	    	    	    
	</LinearLayout>
	<LinearLayout 
	    android:id					= "@+id/timeSlots"
	    android:layout_width		= "match_parent"
	    android:layout_height		= "wrap_content"
	    android:orientation			= "horizontal"
	>
	    <!-- timeSlots for program -->
	    
	</LinearLayout>
   	<LinearLayout 
	    android:layout_width		= "match_parent"
	    android:layout_height		= "wrap_content"
	    android:orientation			= "horizontal"
	>
	    <!-- start/end times for clear reading -->
   	    <TextView
	        android:id				= "@+id/timeStart"
	        style					= "@style/AD_Data"
  			android:gravity			= "left"
        	android:text			= "Start"
	    	android:layout_width	= "100dp"
    	/>
		<TextView
	        android:id				= "@+id/tempObjective"
	        style					= "@style/AD_Data"
  			android:gravity			= "center_horizontal"
        	android:text			= "Temp Objective"
	    	android:layout_width	= "88dp"
	    />
   	    <TextView
	        android:id				= "@+id/timeEnd"
	        style					= "@style/AD_Data"
   			android:gravity			= "right"
        	android:text			= "End"
	    	android:layout_width	= "100dp"
    	/>
	</LinearLayout>
	<LinearLayout 
	    android:layout_width		= "288dp"
	    android:layout_height		= "wrap_content"
	    android:orientation			= "horizontal"
	>
	    <!-- Black seperator + Empty Line -->

      	<TextView
      	    style					= "@style/AD_Text"
      	    android:layout_width	= "288dp"
      	    android:layout_height	= "8dp"
      	    android:background		= "@color/Black"
      	    android:text			= ""
     	/>

      	<TextView
	        style					= "@style/AD_Text"
        	android:text			= ""
	    	android:layout_width	= "200dp"
	    />
	    
	</LinearLayout>
		
</LinearLayout>
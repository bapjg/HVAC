<?xml version		= "1.0" encoding		= "utf-8"?>
<LinearLayout 
    xmlns:android							= "http://schemas.android.com/apk/res/android"
    style									= "@style/AD_Linear_Top"
>
    <include 
        layout								= "@layout/aa_include_panel_title_section"
        android:id							= "@+id/title_layout"
    />

    <TableLayout style						= "@style/AD_Table" >
        <TableRow style						= "@style/AD_Row" >
            <TextView
                style						= "@style/AD_Text"
                android:text				= "Obtained"
			/>
            <TextView
                android:id					= "@+id/dateTimeObtained"
                style						= "@style/AD_Data"
                android:text				= ""
			/>
        </TableRow>
    </TableLayout>
    <ListView
        android:id							= "@+id/List_View"
        android:layout_width				= "match_parent"
        android:layout_height				= "wrap_content"
	/>
</LinearLayout>                 			
                                			